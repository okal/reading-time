{
    "name": "Reading time for Chrome",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Get the estimated reading time before reading a post.",
    "permissions": [
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["custom_style.js"],
            "all_frames": true
        }
    ]
}
